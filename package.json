{
  "name": "taskly",
  "version": "1.0.0",
  "description": "Task app",
  "main": "backend/server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd backend && npm start",
    "client": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "install-deps": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "start": "cd backend && npm start",
    "test": "cd backend && npm test && cd ../frontend && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suletetes/taskly.git"
  },
  "author": "suleiman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/suletetes/taskly/issues"
  },
  "homepage": "https://github.com/suletetes/taskly#readme",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "axios": "^1.12.2"
  }
}
